FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      gradTSol;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 1 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    lowerWall
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    atmosphere
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    rightWall
    {
        type            empty;
    }
    leftWall
    {
        type            empty;
    }
    frontAndBack
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


