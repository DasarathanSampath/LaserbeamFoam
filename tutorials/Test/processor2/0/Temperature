FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Temperature;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    lowerWall
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    atmosphere
    {
        type            zeroGradient;
    }
    rightWall
    {
        type            empty;
    }
    leftWall
    {
        type            empty;
    }
    frontAndBack
    {
        type            fixedValue;
        value           nonuniform 0();
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary2to4
    {
        type            processor;
        value           uniform 300;
    }
}


