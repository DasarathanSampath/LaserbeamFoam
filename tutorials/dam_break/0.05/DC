FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0.05";
    object      DC;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 -1 0 0 0 0];

internalField   uniform 1e+14;

boundaryField
{
    leftWall
    {
        type            calculated;
        value           uniform 1e+14;
    }
    rightWall
    {
        type            calculated;
        value           uniform 1e+14;
    }
    lowerWall
    {
        type            calculated;
        value           uniform 1e+14;
    }
    atmosphere
    {
        type            calculated;
        value           uniform 1e+14;
    }
    defaultFaces
    {
        type            empty;
    }
}


// ************************************************************************* //
