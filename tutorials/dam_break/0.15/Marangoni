FoamFile
{
    format      binary;
    class       volVectorField;
    location    "0.15";
    object      Marangoni;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -2 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    leftWall
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    rightWall
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    lowerWall
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    atmosphere
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    defaultFaces
    {
        type            empty;
    }
}


// ************************************************************************* //
