FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    lowerWall
    {
        type            zeroGradient;
    }
    atmosphere
    {
        type            zeroGradient;
    }
    rightWall
    {
        type            fixedGradient;
        gradient        uniform 299.975;
    }
    leftWall
    {
        type            fixedGradient;
        gradient        uniform 299.975;
    }
    frontAndBack
    {
        type            fixedGradient;
        gradient        uniform 299.95;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 300;
    }
}


