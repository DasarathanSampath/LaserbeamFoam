FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    lowerWall
    {
        type            slip;
    }
    atmosphere
    {
        type            pressureInletOutletVelocity;
        tangentialVelocity 
        {
            type            constant;
            value           (0 0 0);
        }
        value           nonuniform List<vector> 0();
    }
    rightWall
    {
        type            slip;
    }
    leftWall
    {
        type            slip;
    }
    frontAndBack
    {
        type            slip;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


// ************************************************************************* //
