FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Laser_boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    lowerWall
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    atmosphere
    {
        type            fixedValue;
        value           uniform 1;
    }
    rightWall
    {
        type            fixedValue;
        value           uniform -1;
    }
    leftWall
    {
        type            fixedValue;
        value           uniform -1;
    }
    frontAndBack
    {
        type            fixedValue;
        value           uniform -1;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 0;
    }
}


