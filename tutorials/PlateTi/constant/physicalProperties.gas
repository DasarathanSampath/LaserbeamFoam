FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

viscosityModel  constant;

nu              2.8e-05;

rho             1.78;

    cp  520;
cpsolid 520;
    kappa  0.04;
kappasolid 0.04;
    Tsolidus    1.0;
    Tliquidus   10.0;
    LatentHeat  1;
    beta    4.0e-5;

